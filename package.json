{
  "name": "dns",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "test": "tsc --noEmit",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p . && rimraf dist/tsconfig.tsbuildinfo",
    "tsc": "tsc -p .",
    "db:ui": "erd-ui erd.txt",
    "db:setup": "npm run db:migrate",
    "db:dev": "run-s db:migrate db:plan db:update",
    "db:migrate": "knex migrate:latest",
    "db:plan": "auto-migrate db.sqliet3 < erd.txt",
    "db:update": "run-s db:migrate db:gen-proxy",
    "db:gen-proxy": "erd-to-proxy < erd.txt > src/proxy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/dns-packet": "^5.6.5",
    "@types/express": "4",
    "@types/integer": "^4.0.3",
    "@types/node": "^22.9.0",
    "npm-run-all": "^4.1.5",
    "quick-erd": "^4.26.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "better-sqlite3": "^11.2.1",
    "better-sqlite3-proxy": "^2.10.0",
    "better-sqlite3-schema": "^3.1.7",
    "dns-packet": "^5.6.1",
    "dohdec": "^6.0.4",
    "dotenv": "^16.4.5",
    "express": "4",
    "knex": "^3.1.0",
    "listening-on": "^2.0.9",
    "populate-env": "^2.3.1"
  }
}
